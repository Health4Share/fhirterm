{
  "env": "production",
  "http": {
    "port": 7654
  },
  "db": {
    "classname": "org.postgresql.Driver",
    "subprotocol": "postgresql",
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "subname": "//DB_HOST:DB_PORT/DB_NAME"
  },
  "log": {
    "file": "log/fhirterm.log",
    "level": "debug"
  },
  "fhir-client": {
    "protocol": "rest",
    "base-url": "http://localhost:3000/"
  }
}
